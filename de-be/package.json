{
  "name": "dsp-core-server",
  "version": "0.0.0",
  "private": true,
  "apidoc": {
    "name": "Documentation DSP rest api",
    "version": "0.1.0",
    "description": "Description",
    "title": "Ubidex apiDoc",
    "url": "https://api.domen.com"
  },
  "scripts": {
    "start": "node ./bin/www",
    "apidoc": "apidoc -i ./src/ -o apidoc/",
    "create-migration": "cd src/ && ../node_modules/.bin/sequelize migration:generate --name",
    "db-migrate": "cd src/ && babel-node ../node_modules/.bin/sequelize db:migrate",
    "revert-migration": "cd src/ && babel-node ../node_modules/.bin/sequelize db:migrate:undo"
  },
  "_moduleAliases": {
    "@root": ".",
    "@config": "./config",
    "@controllers": "./src/controllers/",
    "@repositories": "./src/repositories/"
  },
  "dependencies": {
